page{
  background-color: #f5f5f5;
}
.wrap{
  background-color: #fff;
}
.line-box{
  width: 267px;
  margin: 0 auto;
  padding: 18px 0;
  text-align: center;
  background-color: #fff;
}
.line-text{
  font-size: 12px;
  color: #909090;
  /* margin: 0 12px;
  float: left; */
}
.line-one{
  display: block;
  width: 88px;
  border: 1rpx #f7f7f7 solid;
  float: left;
  margin-top: 13px;
}
.line-two{
  display: block;
  width: 88px;
  border: 1rpx #f7f7f7 solid;
  float: right;
  margin-top: 13px;
}

.carNum-box{
  border-bottom: 20px #f5f5f5 solid;
  padding-bottom: 20px;
}

.carNum{
  font-size: 36px;
  font-weight: bold;
  text-align: center;
  line-height: 40px;
  margin-bottom: 10px;
}

.prompt_box{
  line-height: 16px;
  text-align: center;
}

.prompt_ico{
  width: 16px;
  height: 16px;
  /* float: left; */
  vertical-align: middle;
  margin-right: 5px;
}
.prompt_info{
  font-size: 12px;
  color: #909090;
  /* float: left; */
}

.address,.plate,.time,.money,.ticket{
  height: 44px;
  font-size: 16px;
  color: #202020;
  line-height: 45px;
  border-bottom: 1rpx #f7f7f7 solid;
  padding: 0 14px 0 20px;
}
.border-none{
  border:none;
}
.plate > span,.ticket > span{
  font-size: 14px;
  color: #909090;
  float: right;
}
.time > span,.money > span{
  font-size: 14px;
  color: rgb(50, 181, 28);
  float: right;
}
.time_input{
  width: 150px;
  font-size: 14px;
  color: rgb(50, 181, 28);
  text-align: right;
  float: right;
  margin: 10px 2px 0 0;
}

.footer{
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 48px;
  font-size: 16px;
  color: #202020;
  line-height: 48px;
  background-color: #fff;
  
}
.money_all{
  margin-left: 20px;
  float: left;
}
.pay_btn{
  width: 140px;
  height: 48px;
  color: #fff;
  text-align: center;
  line-height: 48px;
  background-color: #32b51c;
  float: right;
}