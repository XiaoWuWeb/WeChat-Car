page{
  display: flex;  
  flex-direction: column;  
  height:100%;
  width:100%;
}
.detail-container{
    flex-direction:column;
    justify-content: center;
    align-items: center;
    padding: 30rpx;
}

.parking-name{
    color: white;
    font-size: 20px;
    margin: 15rpx;
    position: absolute;
    bottom: 20rpx;
    left: 20rpx;
    
}

.time{
    color: lightslategrey;
    font-size: 14px;
}

.swiper-img{
    
    height: 500rpx;
    width: 100%;
}

.slide-image{
    filter: blur(5rpx);
    width: 100%;
    height:500rpx;
}

.info-list-container{
  border-top: none;
}

.info-list-content{
  border: none;
}

.info-list-value{
  max-width: 240px;
  overflow: hidden;  
  text-overflow: ellipsis;  
  white-space: nowrap;  
  line-height: 36rpx; 
}

.lock-img{
  display: block;
  width: 125px;
  height: 125px;
  margin: 40px auto 5px;
}
.lock-info{
  font-size: 14px;
  text-align: center;
  color: #333;
}