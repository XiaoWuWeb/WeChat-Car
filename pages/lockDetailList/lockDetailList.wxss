@keyframes slidown{
  from{
    transform:  translateY(-100%);
  }
  to{
    transform:  translateY(0%);
  }
}
.slidown{
  display: block;
  animation: slidown .3s ease-in both;
}
@keyframes slidup{
  from{
    transform:  translateY(0%);
  }
  to{
    transform:  translateY(-100%);
  }
}
.slidup{
  display: none;
  animation: slidup .3s ease-in both;
}

.disappear{
  display: none;
}
.on{
  background-color: #f9f9f9;
}

